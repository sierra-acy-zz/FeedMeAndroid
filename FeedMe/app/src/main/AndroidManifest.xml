<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sierraacy.feedme">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/rooster_logo_96"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditRestaurant"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".AddRestaurant" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".AdvancedFilters" />
        <activity android:name=".LocationsActivity"></activity>
    </application>

</manifest>